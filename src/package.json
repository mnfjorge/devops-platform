{
  "name": "platform-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "build": "rm -rf dist && webpack --mode production --config webpack.config.client.js && webpack --mode production --config webpack.config.server.js",
    "dev": "nodemon --exec 'babel-node -r node_modules/dotenv/config' --watch server server/index.js",
    "test": "jest"
  },
  "dependencies": {
    "connect-redis": "^3.4.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "express-session": "^1.16.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^16.8.6",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.9.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.4.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {},
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "browsers": [
              "last 1 chrome version"
            ]
          }
        }
      ],
      [
        "@babel/react",
        {
          "target": "web"
        }
      ]
    ],
    "plugins": [
      "css-modules-transform"
    ]
  }
}
